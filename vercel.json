{
  "version": 2,
  "functions": {
    "api/quotes/*.go": {
      "runtime": "vercel-go@0.5.0"
    }
  },
  "routes": [
    {
      "src": "^/api/?$",
      "dest": "/api/quotes"
    },
    {
      "src": "^/api/quotes/?$",
      "dest": "/api/quotes"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/quotes"
    },
    {
      "src": "/data/.*",
      "status": 404
    },
    {
      "src": "/assets/.*",
      "status": 404
    }
  ]
}