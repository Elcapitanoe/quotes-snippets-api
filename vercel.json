{
  "version": 2,
  "functions": {
    "api/quotes.js": {
      "runtime": "edge",
      "regions": ["sin1"],
      "memory": 128
    }
  },
  "crons": [
    {
      "path": "/api/quotes?warmup=true",
      "schedule": "*/5 * * * *"
    }
  ],
  "routes": [
    {
      "src": "^/assets/quotes\\.min\\.json$",
      "status": 404
    },
    {
      "src": "^/api(?:/.*)?$",
      "dest": "/api/quotes.js"
    },
    {
      "src": "/assets/.*\\.(txt|json)$",
      "status": 404
    }
  ]
}
