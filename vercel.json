{
  "version": 2,
  "builds": [
    {
      "src": "api/quotes.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/api/quotes.ts"
    },
    {
      "src": "/data/.*",
      "status": 404
    },
    {
      "src": "/assets/.*",
      "status": 404
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "functions": {
    "api/quotes.ts": {
      "maxDuration": 10
    }
  }
}